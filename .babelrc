{
  "presets": [
    ["@babel/preset-env", {
      "modules": false
    }]
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", {
        "legacy": false,
        "decoratorsBeforeExport": true
    }],
    ["@babel/plugin-proposal-class-properties", { "loose": false }]
  ],
  "exclude": [],
  "env": {
    "test": {
      "presets": [["@babel/preset-env"]]
    }
  }
}
